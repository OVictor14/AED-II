#include <stdio.h>
#include <locale.h>

void ciclo(int n, int limite){
		printf("%d\n",n);
		if (n< limite){
			ciclo(n+1,limite);
		}
		
	}

int main(){
	setlocale(LC_ALL,"Portuguese");
	ciclo(2,10);
}
